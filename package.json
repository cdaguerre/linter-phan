{
  "name": "linter-phan",
  "main": "./lib/main",
  "version": "2.0.0",
  "description": "Lint PHP on the fly, using phan",
  "keywords": [
    "lint",
    "linter",
    "phan",
    "php"
  ],
  "bundleDependencies": false,
  "bugs": {
    "url": "https://github.com/cdaguerre/linter-phan/issues"
  },
  "homepage": "https://github.com/cdaguerre/linter-phan#readme",
  "repository": "https://github.com/cdaguerre/linter-phan",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "atom-linter": "^10.0.0",
    "atom-package-deps": "^4.0.1"
  },
  "configSchema": {
    "numProcesses": {
      "type": "integer",
      "default": 1,
      "description": "Number of parallel processes to run for daemon.",
      "order": 1
    }
  }
}
